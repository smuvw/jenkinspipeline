pipeline{
agent any

stages{

   stage('Comple Stage'){
 
      steps{
       { 
             sh 'mvn clean compile'
         }


}
}
stage('Deploy Stage'){
 
      steps{
       { 
             sh 'mvn test'
         }


}
}

stage('Test Stage'){
 
      steps{
      { 
             sh 'mvn deploy'
         }

}
}

}

}
